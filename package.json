		"@sveltejs/kit": "^2.47.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.0-next.3",
		"@testing-library/user-event": "^14.6.1",
		"@types/jest": "^30.0.0",
		"@types/node": "^24",
		"@vitest/coverage-v8": "^4.0.14",
		"@vitest/ui": "^4.0.14",
		"drizzle-kit": "^0.31.7",
